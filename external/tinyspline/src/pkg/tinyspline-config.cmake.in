@PACKAGE_INIT@
set_and_check(TINYSPLINE_INCLUDE_DIRS "@PACKAGE_TINYSPLINE_INSTALL_INCLUDE_DIR@")
set_and_check(TINYSPLINE_LIBRARY_DIRS "@PACKAGE_TINYSPLINE_INSTALL_LIBRARY_DIR@")
set(TINYSPLINE_BINARY_DIRS "@PACKAGE_TINYSPLINE_INSTALL_BINARY_DIR@")
set(TINYSPLINE_VERSION "@TINYSPLINE_VERSION@")
set(TINYSPLINE_DEFINITIONS "@TINYSPLINE_C_DEFINITIONS@")

include("${CMAKE_CURRENT_LIST_DIR}/tinyspline-targets.cmake")
set(TINYSPLINE_LIBRARIES tinyspline::tinyspline)

mark_as_advanced(
	TINYSPLINE_INCLUDE_DIRS
	TINYSPLINE_LIBRARY_DIRS
	TINYSPLINE_BINARY_DIRS
	TINYSPLINE_VERSION
	TINYSPLINE_DEFINITIONS
	TINYSPLINE_LIBRARIES)

include(FindPackageHandleStandardArgs)
find_package_handle_standard_args(tinyspline
	REQUIRED_VARS
		TINYSPLINE_INCLUDE_DIRS
		TINYSPLINE_LIBRARY_DIRS
		TINYSPLINE_BINARY_DIRS
		TINYSPLINE_LIBRARIES
	VERSION_VAR
		TINYSPLINE_VERSION)
